<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:state_enabled="false">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/flat_blue_20" />
            <corners android:radius="25dp" />
            <size android:width="44dp" android:height="25dp" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/flat_blue_50" />
            <corners android:radius="25dp" />
            <size android:width="44dp" android:height="25dp" />
        </shape>
    </item>
    <item android:state_checked="false" android:state_enabled="false">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/switch_track_checked_disable" />
            <corners android:radius="25dp" />
            <size android:width="44dp" android:height="25dp" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/flat_day_night_text_secondary" />
            <corners android:radius="25dp" />
            <size android:width="44dp" android:height="25dp" />
        </shape>
    </item>
</selector>